<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">

	<!-- display non-link (locked and unlocked) -->
	<ui:VisualElement name="display" class="library-attribute-element">
		<ui:Label name="attribute-name" />
	</ui:VisualElement>

	<!-- edit (both link and non-link) -->
	<ui:VisualElement name="edit" class="library-attribute-edit-element hidden">
		<VisualPinball.Unity.Editor.SearchSuggest name="attribute-name-edit" />
		<ui:Button name="cancelButton" text="✕" />
		<ui:Button name="okButton" text="✔" />
	</ui:VisualElement>
</ui:UXML>
